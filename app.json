{
  "expo": {
    "name": "Daily Planner AI",
    "slug": "daily-planner-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/main-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucaspaiva.dailyplannerai",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.lucaspaiva.dailyplannerai",
      "adaptiveIcon": {
        "foregroundImage": "./assets/main-icon-foreground.png",
        "backgroundColor": "#667eea"
      }
    },
    "web": {
      "favicon": "./assets/main-icon.png"
    },
    "plugins": [
      "expo-font",
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "32dcb8e9-3a55-44c4-ac5c-cc8e45525c7b"
      }
    },
    "owner": "lucaspcosta"
  }
}
